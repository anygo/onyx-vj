<settings>

	<!-- CORE SETTINGS -->
	<core>

		<render>
			<!-- the width / height of bitmaps -->
			<bitmapData>
				<width>320</width>
				<height>240</height>
			</bitmapData>
			
			<!-- quality of flash -->
			<quality>high</quality>
		</render>
		
		<!-- midi settings -->
		<midi>
			<enabled>false</enabled>
		</midi>
		
		<!-- custom ordering of blendmodes for blendmode controls -->
		<blendModes>
			<normal/>
			<lighten/>
			<darken/>
			<overlay/>
			<hardlight/>
			<multiply/>
			<screen/>
			<add/>
			<subtract/>
			<difference/>
			<invert/>
		</blendModes>
	</core>

	<!-- FILTER ORDERING -->
	<filters>
		<order>
			<filter name="BLUR FILTER" index="0" />
		</order>
	</filters>
	
	<!-- DEFAULT WINDOW LOCATIONS -->
	<windows>
